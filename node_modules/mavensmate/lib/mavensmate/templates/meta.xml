<?xml version="1.0" encoding="UTF-8"?>
<{{ metadata.type.xmlName }} xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>{{ apiVersion }}</apiVersion>
{% if metadata.type.xmlName === 'ApexPage' or metadata.type.xmlName == 'ApexComponent' %}
	<label>{{ metadata.name }}</label>
	<description></description>
{% endif %}
{% if metadata.type.xmlName === 'ApexTrigger' %}
	<status>Active</status>
{% endif %}
</{{ metadata.type.xmlName }}>
